<div class="board">
  <%= for rank <- 0..7 do %>
    <div class="rank">
      <%= for file <- 0..7 do %>
        <div class="square">
          <%= if get_square(rank, file) != "*" do %>
            <img src={Routes.static_path(@socket, "/images/pieces/#{@assets[get_square(rank, file)]}.svg")} />
          <% end %>
        </div>
      <% end %>
    </div>
  <% end %>
</div>
